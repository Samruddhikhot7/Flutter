//Types of button
//1.Text Button
//2.Elevated Button
//3.Outline Button

child : Column(
            mainAxisAlignment: MainAxisAlignment.center,
            children: [

              Text(                    
            'Hello, Flutter!',style: TextStyle(fontSize: 24,
             color: Colors.blue , fontWeight: FontWeight.bold),),

              TextButton(onPressed: ()
              {
                print("Action is performed");
              },
                onLongPress: (){
                print("Longpressed!");
                },
                child: Text("Button"),

              )
          ]
        ),
